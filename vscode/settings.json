{
    "editor.formatOnSave": true,
    "editor.minimap.enabled": true,
    "editor.minimap.size": "fill",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "all",
    "editor.scrollbar.verticalScrollbarSize": 20,
    "editor.stickyScroll.enabled": true,
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 200,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.timeline.showUncommitted": true,
    "json.format.enable": false,
    "jupyter.askForKernelRestart": false,
    "lammps.AutoComplete.Setting": "None",
    "liveshare.showInStatusBar": "whileCollaborating",
    "Lua.telemetry.enable": false,
    "notebook.cellToolbarVisibility": "hover",
    "notebook.insertToolbarLocation": "betweenCells",
    "notebook.showCellStatusBar": "visibleAfterExecute",
    "problems.showCurrentInStatus": true,
    "python.defaultInterpreterPath": "/usr/local/bin/python3.11",
    "python.interpreter.infoVisibility": "never",
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "settingsSync.ignoredExtensions": [],
    "telemetry.telemetryLevel": "error",
    "terminal.integrated.bellDuration": 10000,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "window.autoDetectColorScheme": true,
    "window.commandCenter": true,
    "window.titleBarStyle": "native",
    "window.zoomLevel": -1,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.layoutControl.enabled": true,
    "workbench.layoutControl.type": "toggles",
    "workbench.tips.enabled": false,
    "workbench.tree.renderIndentGuides": "always",
}